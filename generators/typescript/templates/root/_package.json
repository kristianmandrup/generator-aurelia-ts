{
  "name": "<%= appName %>",
  "version": "0.0.1",
  "description": "<%= appDesc %>",
  "keywords": [
    "aurelia"
  ],
  "bugs": {
    "url": "https://github.com/<%= githubAccount %>/<%= appName %>/issues"
  },
  "license": "MIT",
  "author": "<%= authorName %> <%= authorEmail %>",
  "main": "dist/commonjs/index.js",
  "repository": {
    "type": "git",
    "url": "http://github.com/<%= githubAccount %>/<%= authorName %>"
  },
  "devDependencies": {
    "aurelia-tools": "^0.1.3",
    "browser-sync": "^1.8.1",
    "conventional-changelog": "0.0.11",
    "del": "^1.1.0",
    "gulp": "^3.8.10",
    "gulp-bump": "^0.1.11",
    "gulp-changed": "^1.1.0",
    "gulp-jshint": "^1.9.0",
    "gulp-plumber": "^0.6.6",
    "gulp-protractor": "^0.0.12",
    "gulp-sourcemaps": "^1.3.0",
    "gulp-yuidoc": "^0.1.2",
    "gulp-babel": "^5.1.0",
    "jasmine-core": "^2.1.3",
    "jshint-stylish": "^1.0.0",
    "karma": "^0.12.28",
    "karma-babel-preprocessor": "^5.1.0",
    "karma-chrome-launcher": "^0.1.7",
    "karma-phantomjs-launcher": "^0.1",
    "karma-jasmine": "^0.3.5",
    "karma-nyan-reporter": "^0.1.0",
    "karma-jspm": "^1.1.5",
    "object.assign": "^1.0.3",
    "require-dir": "^0.1.0",
    "run-sequence": "^1.0.2",
    "vinyl-paths": "^1.0.0",
    "yargs": "^2.1.1",
    "merge2": "*",
    "typescript": "Microsoft/Typescript",
    "gulp-typescript": "^2.7.6"
  },
